{
  "manifest_version": 2,

  "name": "GBF Weapon Pool Analyzer",
  "description": "This extension shows the effective damage of your weapon pool in Granblue Fantasy",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "constants.js",
      "background.js"
    ]
  },
  "permissions": [
    "http://game.granbluefantasy.jp/*"
  ]
}
